    "element-plus": "^2.2.32",
    "md-editor-v3": "^2.8.1",
    "pinia": "^2.0.32",
    "qs": "^6.11.0",
    "vite": "^4.1.4",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/echarts": "^4.9.16",
    "@types/node": "^18.14.1",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
